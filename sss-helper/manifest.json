{
    "name": "sss-helper",
    "options_page": "options.html",
    "version": "1.0",
    "description": "Scalable extension for sss-Inspector",
    "permissions": ["tabs", "activeTab", "declarativeContent", "storage"],
    "externally_connectable": {
        "matches": ["*://furkansarihan.github.io/*/*/*"]
    },
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["send-dom-to-background.js"],
          "run_at": "document_end"
        }
      ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "icons": {
    },
    "manifest_version": 2
}